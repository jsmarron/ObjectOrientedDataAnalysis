estSS2<-function (S, weights = 1)
{
M <- dim(S)[3]
k <- dim(S)[1]
H <- defh(k)
if (length(weights) == 1) {
weights <- rep(1, times = M)
}
Q <- array(0, c(k + 1, k, M))
for (j in 1:M) {
Q[, , j] <- t(H) %*% (rootmat(S[, , j]))
}
ans <- procWGPA(Q, fixcovmatrix = diag(k + 1), scale = FALSE,
reflect = TRUE, sampleweights = weights)
t(H %*% ans$mshape) %*% (H %*% ans$mshape)
}
